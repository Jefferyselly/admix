         <div class="row ">
           <div class="col-12 grid-margin">
             <div class="card">
               <div class="card-body">
                 <h4 class="card-title"> Search </h4>
                 <div class="table-responsive">
                   <table class="table">

                     <thead>
                       <tr>
                         <th>
                           <div class="form-check form-check-muted m-0">
                             <label class="form-check-label">
                               <input type="checkbox" class="form-check-input">
                             </label>
                           </div>
                         </th>
                         <th> Username </th>
                         <th> User ID </th>
                         <th> Amount paid </th>
                         
                         <th> Payment Mode </th>
                         
                         <th> Date registered </th>
                       </tr>
                     </thead>
                     <tbody>
                       
                       <tr>
                         <td>
                           <div class="form-check form-check-muted m-0">
                             <label class="form-check-label">
                               <input type="checkbox" class="form-check-input">
                             </label>
                           </div>
                         </td>
                         <td>
                           <img src="assets/images/faces/face1.jpg" alt="image" />
                           <span class="pl-2"><%= user.username %></span>
                         </td>
                         <td> <%= user._id %> </td>
                         <td> <%= user.amount %> </td>
                         <td> <%= user.payment_method %></td>
                         
                         <td> <%= user.date.toLocaleString() %> </td>
                         <td>
                           <a href='/admin/upgrade_user?upg=<%= user._id%>'><div class="badge <%= user.button_class %>" id="<%= user._id %>" onclick=""> <%= user.button_text %> </div></a>
                         </td>
                       </tr>
                     
                     
                     </tbody>
                   </table>
                 </div>
               </div>
             </div>
           </div>
         </div>
        
       
